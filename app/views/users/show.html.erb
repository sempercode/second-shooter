<div class="show">
<div class="container-fluid">	
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<table class="table">
				<tbody>
					<tr>
						<th>Email</th> 
						<td><%= mail_to(@users.email) %> </td>
					</tr>
					<tr>
						<th>Name</th> 
						<td><%= @users.first_name %> </td>
					</tr>
					<tr>
						<th>Website</th> 
						<td><%= link_to(@users.website) %> </td>
					</tr>
					<tr>
						<th>Hourly Rate</th> 
						<td><%= @users.hourly_rate %> </td>
					</tr>
				</tbody>
			</table>	
			<% if user_signed_in? %>
			<%= link_to "Edit / Cancel Account Information", edit_user_registration_path(@users) %>
			<% end %>
		</div>
	</div>
</div>
</div>
